@model Project.Models.Cultural_eventlist
@{
    Layout = null;
}


<text>@ViewBag.PSP</text>


@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <table class="table-responsive">
            <tr>
                <th>
                    DATE:
                </th>
                <th>
                    TIME:
                </th>
                <th>
                    VENUE:
                </th>
            </tr>
            @foreach (var item in ViewBag.ASQ)
            {
                <tr>

                    <td>
                        <text>@item.event_date</text>
                    </td>
                    <td>
                        <text>@item.event_time</text>
                    </td>
                    <td>
                        <text>@item.event_venue</text>
                    </td>
                    <td>



                        @Html.ActionLink("Register Individually", "addini", new { id = @item.cul_evid }, new { @class = "GetResult" })

                    </td>
                </tr>
            }

        </table>
    </div>
}







